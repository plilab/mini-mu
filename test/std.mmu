def lt a b k :=
  (a, b) . { (Z, Z) -> False . k
           | (Z, S _) -> True . k
           | (S a', Z) -> False . k
           | (S a', S b') -> lt @ a' b' k };

def geq a b k :=
  lt @ b a k;

def list_append as bs k :=
  as . { Nil -> bs . k
       | List:: a as' -> list_append @ as' bs { rest -> List:: a rest . k } }

export lt, geq, list_append