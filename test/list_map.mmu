list_map = comu[ ~Ap3 f xs ~k -> 
                < xs |> mu[ Nil -> < Nil |> ~k > 
                          | List:: x xs' -> 
                            < f |> ~Ap x mu[ y -> 
                                < list_map |> ~Ap3 f xs' mu[ ys' -> 
                                     < List:: y ys' |> ~k > ] > ] > ] > ];

inc = comu[ ~Ap x ~k -> < S x |> ~k > ];

main = comu[ ~Halt -> 
    < list_map |> ~Ap3 inc (List:: (S (S Z)) (List:: (S Z) (List:: Z Nil))) ~Halt > ]

